<view class="list_box" hidden="{{search_Bool}}">
  <form>
  <view class="search-box">
      <input class="search-input" type="search" name="searchKey" placeholder="搜索店铺名称" placeholder-class='color' value="{{search_Text}}" bindinput='getFocus'/>
      <view class="clear" bindtap="clearInput"><icon wx:if="{{search_Text.length>0}}" type="clear" size="10"></icon></view>
      <view class="search-btn" form-type="submit" bindtap="onSearch"><image src='../../img/zoom.png'></image></view>
  </view>
  </form>
  <view class="layout_LR_Box">

    <block wx:for="{{goods}}" wx:key="{{index}}">
      <view class="layout_LR">
        <view class="layout_Left">{{item.m_store_name}}</view>
        <view class="layout_Right" wx:if="{{item.m_status==1}}"><image src="/img/limg.png"></image></view>
        <view class="layout_Right" wx:else>
          <view class="import_Btn" bindtap="onToggle">立即输入</view></view>
      </view>
    </block>

    <view class="layout_LR" wx:if="{{goods.length==0}}">
      <view class="layout_Left">改商家还未入驻</view>
      <view class="layout_Right">
        <view class="import_Btn" bindtap="onToggle">立即入驻</view></view>
    </view>
  </view>
</view>


<view class='form' hidden="{{form_Bool}}">
  <form bindsubmit="ch_Register">
    <view class='list cf'>
      <text class='fl text'>姓名</text>
      <view class='fl input'><input type='text' name="m_name" placeholder="请输入姓名"/></view>
    </view>
    <view class='list cf'>
      <text class='fl text'>手机号</text>
      <view class='fl input'><input type='number' name="m_phone" placeholder="请输入手机号" maxlength="11"/></view>
    </view>
    <!--<view class='list  cf'>-->
      <!--<text class='fl text'>验证码</text>-->
      <!--<view class='fl input'><input type='number'/></view>-->
      <!--<view class='bnt fr'>获取手机验证码</view>-->
    <!--</view>-->
    <view class='list cf'>
      <text class='fl text'>微信号</text>
      <view class='fl input'><input type='text' name="m_openid" placeholder="请输入微信号"/></view>
    </view>
    <view class='list cf'>
      <text class='fl text'>产品名称</text>
      <view class='fl input'><input type='text' name="m_product_id" placeholder="请输入产品名称" maxlength="20"/></view>
    </view>
    <view class='list cf'>
      <text class='fl text'>店名</text>
      <view class='fl input'><input type='text' name="m_store_name" placeholder="请输入店名"/></view>
    </view>
    <view class='list cf' bindtap="chooseLocation">
      <text class='fl text'>店铺地址</text>
      <view class="fl input" >
        <input type='text' name="m_address" placeholder="点击选择位置" value="{{d_address}}" disabled="true"/>
      </view>
      <!--<view class="province cf fl" focus="{{focus}}" bindtap="open">-->
        <!--<view class='province_list'>{{province}}</view>-->
        <!--<view class='province_list'>{{city}}</view>-->
        <!--<view class='province_list'>{{county}}</view>-->
      <!--</view>-->
      <!--<image class='fr xl' src='../../img/xl.png'></image>-->
    </view>
    <view class='list cf'>
      <text class='fl text'>详细地址</text>
      <view class='fl input'><input type='text' name="m_address_detailed" placeholder="请输入详细地址" maxlength="20"/></view>
    </view>
    <view class='down'>
      <button class='big_bnt' formType="submit">免费获取</button>
      <!--<view>（验证后立即发货）</view>-->
    </view>
  </form>
</view>
<view wx:if="{{condition}}" class="citypicker">
  <picker-view indicator-style="height: 50px;" style="width: 100%; height: 300px;" value="{{value}}" bindchange="bindChange" class="citybody">
    <view class="cityheader">
      <view bindtap="open" class="city-cancel">取消</view>
      <view bindtap="open" class="city-true">确定</view>
    </view>
    <picker-view-column>
      <view wx:for="{{provinces}}" wx:key="item" style="line-height: 50px;padding-left:10px;">{{item}}</view>
    </picker-view-column>
    <picker-view-column>
      <view wx:for="{{citys}}" wx:key="item" style="line-height: 50px;padding-left:10px;">{{item}}</view>
    </picker-view-column>
    <picker-view-column>
      <view wx:for="{{countys}}" wx:key="item" style="line-height: 50px;padding-left:10px;">{{item}}</view>
    </picker-view-column>
  </picker-view>
</view>