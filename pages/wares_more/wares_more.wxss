swiper {
  display:block;
  height:490rpx;
  border-bottom: 1px solid #ddd;
}
swiper image {
  display:block;
  width: 100%;
  height:490rpx;
}
.header{
  height: 123rpx;
  padding: 0 3%;
}
.header .header_l{
  margin-top: 20rpx;
}
.header .header_l text{
  display: block;
  font-size: 28rpx;
}
.header .header_l .state{
  color: #F46366;
}
.header .header_r{
  height: 123rpx;
  line-height: 123rpx;
}
.header .header_r image{
  width: 35rpx;
  height: 35rpx;
  margin-top: 43rpx;
}
.header .header_r text{
  font-size: 28rpx;
  color: #666;
  padding-left: 10rpx;
}
.profile view{
  padding: 4% 3%;
  font-size: 24rpx;
  color: #ccc;
  line-height: 170%;
}
.profile view text{
  color: #666;
}
.profile view:first-child{
  border-bottom: 1px solid #ddd;
}
.concent{
  margin-bottom: 100rpx;
}
.concent .top{
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 3%;
}
.concent .top text{
  font-size: 32rpx;
  padding-left: 20rpx;
  border-left: 3px solid #709EF4;
}
.concent .img_box image{
  width: 100%;
  display: block;
}
.fixed_nav .free{
  width: 100%;
  background-color: #F47070;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
}
.share{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
}
.share .mask{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
}
.share_box{
  background-color: #fff;
  width: 85%;
  position: absolute;
  padding: 34rpx 0;
}
.share_box .logo .userinfo-avatar {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
  margin-left: 40rpx;
}
.share_box .logo text{
  color: #333;
  display: block;
  font-size: 28rpx;
  margin-top: 24rpx;
  margin-left: 30rpx;
}
.share_box .logo .gsname{
  width: 348rpx;
  color: #999;
}
.share_box .cp{
  width: 90%;
  height: 490rpx;
  margin: 10rpx auto 20rpx auto;
  display: block;
}
.share_box .concent{
  width: 90%;
  margin: 0 auto 40rpx auto;
}
.share_box .concent .left .text1{
  color: #000;
  font-size: 28rpx;
}
.share_box .concent .left .text2{
  color: #F46366;
  font-size: 28rpx;
  margin-top: 10rpx;
}
.share_box .concent .left .text3{
  color: #999;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.share_box .concent .right{
  width: 140rpx;
  height: 140rpx;
}
.share_bnt{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background-color: #709EF4;
  margin: 30rpx auto 0 auto;
  border-radius: 0;
}
.xq_concent view{
  font-size: 28rpx;
  line-height: 180%;
  color: #999;
  text-align: justify;
  margin-bottom: 20rpx;
}
.xq_concent image{
  width: 100%;
}
.announcement{
   position:fixed;
  top:400rpx;
  left:20rpx;
  text-align: center;
  padding:20rpx 0;
  opacity:0.5;
  filter:alpha(opacity=50);
  color:#F46366;
  font-size:30rpx;
}