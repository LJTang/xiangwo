.top_nav{
  height: 60rpx;
  line-height: 60rpx;
  background-color: #709EF4;
}
.top_nav .list{
  width: 20%;
  color: #fff;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
.screen{
  position: relative;
  z-index: 22;
}
.screen .down1{
  position: absolute;
  top: 0;
  left: 257rpx;
}
.screen .down2{
  position: absolute;
  top: 0;
  right: 35rpx;
}
.screen .drop-down{
  width: 200rpx;
  background-color: #fff;
  text-align: center;
  line-height: 60rpx;
  padding-bottom: 20rpx;
}
.screen .drop-down view{
  font-size: 28rpx;
  color: #333;
}
.screen .drop-down view.active{
  color: #709EF4;
}
.top_nav .list image{
  width: 13rpx;
  height: 11rpx;
  margin-left: 10rpx;
}
.publicity .list_top .logo{
  width: 86rpx;
  height: 86rpx;
  border: 1px solid #ddd;
}
.publicity .list_top .names{
  margin-left: 20rpx;
}
.publicity .list_top .names .name{
  font-size: 28rpx;
  color: #000;
}
.publicity .list_top .names .time{
  font-size: 24rpx;
  color: #999;
  margin-top: 20rpx;
}
.publicity .list_top .fenx image{
  width: 45rpx;
  height: 45rpx;
}
.publicity .list_top .fenx view{
  font-size: 24rpx;
  color: #999;
}
.publicity .concent .topic{
  font-size: 28rpx;
  color: #709EF4;
  margin: 31rpx 0 15rpx 0;
  text-align: justify
}
.publicity .concent .topic text{
  color: #666;
}
.publicity .concent .img_box image{
  width: 240rpx;
  height: 345rpx;
  border: 1px solid #ddd;
}
.publicity .concent .img_boxs image{
  width: 219rpx;
  height: 237rpx;
  margin-bottom: 20rpx;
}
.publicity .concent .video_box{
  width: 100%;
  height: 364rpx;
  position: relative;

}
.publicity .concent .video_box .img1{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 364rpx;
  display: block;
}
.publicity .concent .video_box .img2{
  width: 100%;
  height: 364rpx;
  display: block;
}
.publicity .concent .video_box .video{
  width: 100%;
  height: 364rpx;

}
.publicity .concent .video_box video.goleft{
  position: absolute;
  top: 0;
  left: -10000px;
} 
.mask{
  position: fixed;
  width: 100%;
  top: 60rpx;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
  z-index: 11;
}