              <image src="{{item.img}}" class="left fl"/>
              <view class='fl right'><text class='name '>{{item.name}}</text><text class='time'>业务员：{{item.time}}</text></view>
              <view class='price'>￥{{item.price}}</view>
              <view class='numbers'>{{item.numbers}}</view>
            </navigator>
          </view>
        </block>
      </view>
    </view>
  </view>
