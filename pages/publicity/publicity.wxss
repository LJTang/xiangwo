.screen_Box{
  display: flex;
  flex-direction: row;
  width:710rpx;
  height:50px;
  border:1rpx solid #d9d9d9;
  margin:20rpx;
  border-radius:5px;
}
.screen_Box .import{
  width:550rpx;
}
.screen_Box .import input{
  margin-left: 20rpx;
  width:530rpx;
  height:50px;
  line-height:50px;
}
.screen_Box .clear{
  width:60rpx;
  text-align: center;
}
.screen_Box .clear icon{
  margin-top:15px;
}

.screen_Box .search_Btn{
  width:100rpx;
  text-align: center;
}
.screen_Box .search_Btn icon{
  margin-top:10px;
}

.clear{
  width:10px;
}
.clear icon{

}

.search-box{
  width: 710rpx;
  border: 1px solid #ccc;
  height: 80rpx;
  line-height: 80rpx;
  margin: 20rpx auto;
  border-radius: 4px;
  display: flex;
  flex-direction:row;

}
.search-box input{
  width: 82%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  padding-left:30rpx;
}
.search-box .search-btn{
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 0;
  padding: 0;
  border-radius: 0;
  /* position: absolute; */
  /* top: 0; */
  /* right: 0; */
}
.search-box .search-btn image{
  width: 30rpx;
  height: 30rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
}


.top_nav{
  height: 60rpx;
  line-height: 60rpx;
  background-color: #709EF4;
}
.top_nav .list{
  width: 20%;
  color: #fff;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
.screen{
  position: relative;
  z-index: 22;
}
.screen .down1{
  position: absolute;
  top: 0;
  left: 257rpx;
}
.screen .down2{
  position: absolute;
  top: 0;
  right: 35rpx;
}
.screen .drop-down{
  width: 200rpx;
  background-color: #fff;
  text-align: center;
  line-height: 60rpx;
}
.screen .drop-down view{
  font-size: 28rpx;
  color: #333;
}
.screen .drop-down view.active{
  color: #709EF4;
}
.top_nav .list image{
  width: 13rpx;
  height: 11rpx;
  margin-left: 10rpx;
}
.list_top{padding-top:20rpx;}
.publicity .list_top .logo{
  width: 86rpx;
  height: 86rpx;
  border: 1px solid #ddd;
}
.publicity .list_top .names{
  margin-left: 20rpx;
}
.publicity .list_top .names .name{
  font-size: 28rpx;
  color: #000;
}
.publicity .list_top .names .time{
  font-size: 24rpx;
  color: #999;
  margin-top: 20rpx;
}
.publicity .list_top .fenx image{
  width: 45rpx;
  height: 45rpx;
}
.publicity .list_top .fenx view{
  font-size: 24rpx;
  color: #999;
}
.publicity .concent .topic{
  font-size: 28rpx;
  color: #709EF4;
  margin-top:30rpx;
  margin-bottom: 15rpx;
}
.publicity .concent .img_box image{
  width: 240rpx;
  height: 345rpx;
  border: 1px solid #ddd;
}
.publicity .concent .img_boxs{
  display:flex;
  flex-wrap:wrap;
}
.publicity .concent .img_boxs image{
  width: 223.33rpx;
  height: 237rpx;
  margin-bottom: 20rpx;
  margin-left:20rpx;
}
.mask{
  position: fixed;
  width: 100%;
  top: 60rpx;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
  z-index: 11;
}

.mal20R{margin-left:20rpx;}
