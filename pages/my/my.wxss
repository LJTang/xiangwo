.tops{
  position: relative;
  height: 224rpx;
  width: 100%;
  overflow: hidden;
}
.tops .back{
  width: 100%;
  height: 240rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.userinfo {
  height: 224rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.userinfo-avatar {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
  margin-left: 40rpx;
}

.userinfo-nickname {
  color: #fff;
  font-size: 32rpx;
  margin-left: 30rpx;
}

.tops button{
  width: 200rpx;
  height: 60rpx;
  background-color: #fff;
  text-align: center;
  font-size: 28rpx;
  line-height: 60rpx;
}
.my_lists navigator,.my_lists .first{
  height: 105rpx;
  line-height: 105rpx;
  border-bottom: 1px solid #f1f1f1;
  padding: 0 4%;
}

.my_lists navigator .my_left,.my_lists .first .my_left{
  width: 40rpx;
  height: 40rpx;
  margin-top: 32rpx;
}
.my_lists navigator .my_text,.my_lists .first .my_text{
  font-size: 28rpx;
  color: #666;
  margin-left: 20rpx;
}
.my_lists navigator .my_right,.my_lists .first .my_right{
  width: 14rpx;
  height: 28rpx;
  margin-top: 40rpx;
}
.row_lists{
  height: 186rpx;
}
.row_lists navigator,.row_lists .first{
  width: 25%;
  text-align: center;
  position: relative;
}
.row_lists navigator image,.row_lists .first image{
  width: 51rpx;
  height: 47rpx;
}
.row_lists navigator view{
  font-size: 28rpx;
